<Page x:Class="MaquetaParaFinal.View.Index"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MaquetaParaFinal.View"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Index">

    <StackPanel x:Name="Contenedor" HorizontalAlignment="Center" VerticalAlignment="Center">
        <Image x:Name="Logo" Source="/View/logo.png" MaxHeight="250"/>
        <Label Content="Bienvenido al Hospital HILET" HorizontalAlignment="Center" Foreground="#F0F0F0" FontSize="25" FontWeight="Bold"/>
        <Label Content="Servicio de primera calidad desde 1993" HorizontalAlignment="Center" Foreground="#F0F0F0" FontSize="16" FontWeight="SemiBold"/>
    </StackPanel>
</Page>
